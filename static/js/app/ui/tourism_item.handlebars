{{#each items}}
<li><a href="../travel/tourism-detail.html?code={{code}}">
	<div class="img"><img src="{{formatListImage pathPic}}"/></div>
	<div class="title plr10 t-3dot">{{name}}</div>
	<div class="con plr10"><img class="fl" src="/static/images/时间.png"><p class="fl">{{formateDate outDateStart}}~{{formateDate outDateEnd}}</p></div>
	<div class="price plr10"><p><i>￥{{formatMoney price}}</i>起</p></div>
</a></li>
{{/each}}
